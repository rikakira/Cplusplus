#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;

void fun(int &a, int &b) {
    int c, d;
    c = a+b;
    if(a>b)
    {
        d = a-b;
    }
    else { 
        d = b-a;
    }
    a = c;
    b =d;
    
    
}
int main() {
     int a, b;
     int *pa = &a, *pb = &b;
     cin>>a>>b;
     fun(*pa,*pb);
     cout<<a<<"\n"<<b;
    return 0;
}
